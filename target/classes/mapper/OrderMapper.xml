<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jason.classroom.mapper.OrderMapper">
    <select id="selectByNum" resultType="com.jason.classroom.entity.Order">
        SELECT * FROM t_order WHERE num = #{num}
    </select>
</mapper>
